{
    "expo": {
        "name": "Exponent Alerts",
        "slug": "DoneWithIt",
        "platforms": ["ios", "android", "web"],
        "plugins": [
            [
                "expo-notifications",
                {
                    "android": {
                        "sound": true
                    },
                    "color": "#00ff00",
                    "sounds": [
                        "./android/app/src/main/res/beep.mp3"
                    ]
                }
            ]
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "",
        "splash": {
            "image": "",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "package": "com.channel92news.push",
            "googleServicesFile": "./google-services.json"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.channel92news.push"
        },
        "extra": {
            "eas": {
                "projectId": "0dad8d7a-9fe5-40b3-8589-eafca5273909"
            }
        }
    }
}